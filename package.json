{
  "name": "vgg",
  "version": "0.0.10",
  "description": "基于eggjs的vue spa框架。允许以插件的形式添加任何资源（路由，组件，接口，store...）",
  "main": "./src/index.js",
  "scripts": {
    "build": "easywebpack-build && npm run deploy-static",
    "build-uat": "export EGG_SERVER_ENV=unittest && npm run build",
    "build-qa": "export EGG_SERVER_ENV=qa && npm run build",
    "build-stage": "export EGG_SERVER_ENV=stage && npm run build",
    "build-product": "export EGG_SERVER_ENV=prod && npm run build",
    "lint": "eslint .",
    "test": "npm run lint -- --fix && egg-bin test",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "start-bg": "npm run online && eggctl start --daemon",
    "start": "npm run online && eggctl start",
    "stop": "eggctl stop",
    "offline": "heartcheck-offline",
    "online": "heartcheck-online"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "chokidar": "^2.0.4",
    "fs-extra": "^5.0.0",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.4",
    "toposort-class": "^1.0.1",
    "vue": "^2.5.16",
    "vue-router": "^2.7.0",
    "vuex": "^2.4.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "autod": "^2.8.0",
    "core-js": "^2.5.7",
    "egg": "^1.11.0",
    "egg-bin": "^3.4.0",
    "egg-heartcheck": "^0.0.1",
    "egg-scripts": "^2.5.0",
    "egg-validate": "^1.0.0",
    "egg-vgg": "0.0.2",
    "vgg-webpack": "^0.0.4",
    "vue-template-compiler": "^2.5.16",
    "yargs": "^12.0.2"
  },
  "author": "acthtml"
}
