{
  "name": "vgg",
  "version": "0.0.5",
  "description": "基于eggjs的vue spa框架。允许以插件的形式添加任何资源（路由，组件，接口，store...）",
  "main": "./src/index.js",
  "scripts": {
    "build": "easywebpack-build && npm run deploy-static",
    "build-uat": "export EGG_SERVER_ENV=unittest && npm run build",
    "build-qa": "export EGG_SERVER_ENV=qa && npm run build",
    "build-stage": "export EGG_SERVER_ENV=stage && npm run build",
    "build-product": "export EGG_SERVER_ENV=prod && npm run build",
    "lint": "eslint .",
    "test": "npm run lint -- --fix && egg-bin test",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "start-bg": "npm run online && eggctl start --daemon",
    "start": "npm run online && eggctl start",
    "stop": "eggctl stop",
    "offline": "heartcheck-offline",
    "online": "heartcheck-online"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-polyfill": "^6.26.0",
    "chokidar": "^2.0.4",
    "fs-extra": "^5.0.0",
    "lodash": "^4.17.4",
    "toposort-class": "^1.0.1",
    "vue": "^2.5.8",
    "vue-router": "^2.7.0",
    "vuex": "^2.4.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "autod": "^2.8.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "egg": "^1.11.0",
    "egg-bin": "^3.4.0",
    "egg-ci": "^1.7.0",
    "egg-easyvue": "^0.0.3",
    "egg-easywebpack": "0.0.13",
    "egg-heartcheck": "^0.0.1",
    "egg-scripts": "^2.5.0",
    "egg-validate": "^1.0.0",
    "egg-view-vue": "^1.0.0",
    "egg-webpack": "^3.0.2",
    "eslint": "^4.12.0",
    "eslint-config-egg": "^4.2.0",
    "vue-template-compiler": "^2.5.8",
    "yargs": "^12.0.2"
  },
  "author": "acthtml"
}
