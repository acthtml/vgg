{
  "name": "vgg",
  "version": "0.1.2",
  "description": "基于eggjs的vue spa框架。允许以插件的形式添加任何资源（路由，组件，接口，store...）",
  "main": "./src/index.js",
  "scripts": {
    "build": "easywebpack-build && npm run deploy-static",
    "build-uat": "cross-env EGG_SERVER_ENV=unittest npm run build",
    "build-qa": "cross-env EGG_SERVER_ENV=qa npm run build",
    "build-stage": "cross-env EGG_SERVER_ENV=stage npm run build",
    "build-product": "cross-env EGG_SERVER_ENV=prod npm run build",
    "lint": "eslint .",
    "test": "npm run lint -- --fix && egg-bin test",
    "dev": "cross-env EGG_SERVER_ENV=local egg-bin dev",
    "debug": "egg-bin debug",
    "start-bg": "npm run online && eggctl start --daemon",
    "start": "npm run online && eggctl start",
    "stop": "eggctl stop",
    "offline": "heartcheck-offline",
    "online": "heartcheck-online"
  },
  "bin": {
    "vgg-build": "./tools/build.js"
  },
  "dependencies": {
    "cross-env": "^5.2.0",
    "axios": "^0.17.1",
    "chokidar": "^2.0.4",
    "fs-extra": "^5.0.0",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.4",
    "toposort-class": "^1.0.1",
    "vue": "^2.5.16",
    "vue-router": "^2.7.0",
    "vuex": "^2.4.1",
    "vuex-router-sync": "^5.0.0",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-async-to-generator": "^7.1.0",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/runtime": "^7.1.2",
    "autoprefixer": "^8.0.0",
    "autoprefixer-loader": "^3.2.0",
    "babel-loader": "^8.0.0-beta.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "core-js": "^2.5.7",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "html-loader": "^0.5.1",
    "postcss-loader": "^2.1.1",
    "progress-bar-webpack-plugin": "^1.11.0",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "vue-loader": "^15.4.2",
    "vue-server-renderer": "^2.5.16",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^3.4.1",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.6.0",
    "webpack-tool": "^3.1.4",
    "workbox-webpack-plugin": "^3.6.3"
  },
  "devDependencies": {
    "autod": "^2.8.0",
    "core-js": "^2.5.7",
    "egg": "^1.11.0",
    "egg-bin": "^3.4.0",
    "egg-heartcheck": "^0.0.1",
    "egg-scripts": "^2.5.0",
    "egg-validate": "^1.0.0",
    "egg-vgg": "0.0.2",
    "vue-template-compiler": "^2.5.16",
    "yargs": "^12.0.2"
  },
  "author": "acthtml"
}
